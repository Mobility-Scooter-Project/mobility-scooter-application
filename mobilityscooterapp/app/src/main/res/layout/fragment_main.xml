<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:background="#0000FF"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/hamburgerMenu"
            android:layout_width="52dp"
            android:layout_height="60dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="-1dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/hamberger_button" />

        <TextView
            android:id="@+id/text_above"
            android:layout_width="65dp"
            android:layout_height="26dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="-41dp"
            android:layout_toLeftOf="@id/hamburgerMenu"
            android:gravity="end"
            android:text="@string/more"
            android:textColor="@color/white" />
    </RelativeLayout>


    <RelativeLayout
        android:layout_width="416dp"
        android:layout_height="120dp"
        android:layout_marginTop="200dp"
        android:background="@drawable/drive_bottom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="251dp"
            android:layout_height="100dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-51dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="216dp"
            android:layout_marginBottom="9dp"
            android:src="@drawable/baseline_bike_scooter_24" />

        <Button
            android:id="@+id/Drive_Bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/transparent" />

        <TextView
            android:layout_width="110dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="147dp"
            android:layout_marginEnd="159dp"
            android:layout_marginBottom="51dp"
            android:text="@string/drive"
            android:textSize="32sp" />

        <TextView
            android:layout_width="226dp"
            android:layout_height="53dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="142dp"
            android:layout_marginEnd="48dp"
            android:layout_marginBottom="2dp"
            android:text="@string/record_a_new_session"
            android:textSize="22sp" />
    </RelativeLayout>


    <RelativeLayout
        android:layout_width="416dp"
        android:layout_height="120dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/analytics_bottom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="251dp"
            android:layout_height="100dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-51dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="216dp"
            android:layout_marginBottom="9dp"
            android:src="@drawable/baseline_analytics_24" />

        <Button
            android:id="@+id/analytics_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/transparent" />

        <TextView
            android:layout_width="143dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="135dp"
            android:layout_marginEnd="138dp"
            android:layout_marginBottom="64dp"
            android:text="@string/analytics"
            android:textSize="32sp" />

        <TextView
            android:layout_width="282dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="135dp"
            android:layout_marginEnd="-1dp"
            android:layout_marginBottom="27dp"
            android:text="@string/view_data_analytics_or_history"
            android:textSize="18sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="416dp"
        android:layout_height="120dp"
        android:layout_marginTop="460dp"
        android:background="@drawable/messages_bottom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="251dp"
            android:layout_height="100dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-51dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="216dp"
            android:layout_marginBottom="9dp"
            android:src="@drawable/baseline_email_24" />

        <Button
            android:id="@+id/messages_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/transparent" />

        <TextView
            android:layout_width="206dp"
            android:layout_height="50dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="149dp"
            android:layout_marginEnd="61dp"
            android:layout_marginBottom="33dp"
            android:text="@string/messages"
            android:textSize="32sp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="416dp"
        android:layout_height="120dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="590dp"
        android:background="@drawable/accessibility_setting_bottom">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="251dp"
            android:layout_height="100dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-51dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="216dp"
            android:layout_marginBottom="9dp"
            android:src="@drawable/baseline_accessibility_new_24" />

        <Button
            android:id="@+id/accessibility_setting_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/transparent" />

        <TextView
            android:layout_width="181dp"
            android:layout_height="51dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="147dp"
            android:layout_marginEnd="88dp"
            android:layout_marginBottom="51dp"
            android:text="@string/accessibility"
            android:textColor="@color/white"
            android:textSize="32sp" />

        <TextView
            android:layout_width="274dp"
            android:layout_height="66dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="143dp"
            android:layout_marginEnd="-1dp"
            android:layout_marginBottom="-15dp"
            android:text="@string/edit_accessibility"
            android:textColor="@color/white"
            android:textSize="19sp" />
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>